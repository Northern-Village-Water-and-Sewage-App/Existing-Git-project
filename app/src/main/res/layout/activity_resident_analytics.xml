<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResidentAnalyticsActivity">

    <ProgressBar
        android:id="@+id/progressBarAnalytics2"
        android:layout_width="50dp"
        android:layout_height="229dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="336dp"
        android:layout_marginBottom="201dp"
        android:progressDrawable="@drawable/verticalprogressbar" />

    <ProgressBar
        android:id="@+id/progressBarAnalytics"
        android:layout_width="49dp"
        android:layout_height="258dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="337dp"
        android:layout_marginBottom="473dp"
        android:progressDrawable="@drawable/verticalprogressbar" />

    <ListView
        android:id="@+id/residentUseListView"
        android:layout_width="280dp"
        android:layout_height="445dp"
        android:layout_alignTop="@+id/progressBarAnalytics2"
        android:layout_alignBottom="@+id/progressBarAnalytics"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="-255dp" />

    <Switch
        android:id="@+id/editUseSwitch"
        android:layout_width="95dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="285dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="104dp"
        android:backgroundTint="@color/colorPrimary"
        android:text="@string/edit"
        android:textSize="16sp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/addUseFloatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="208dp"
        android:layout_marginEnd="147dp"
        android:layout_marginBottom="90dp"
        android:clickable="true"
        android:elevation="12dp"
        android:focusable="true"
        android:tint="@color/colorLightText"
        app:backgroundTint="@color/colorPrimary" />

    <TextView
        android:id="@+id/myUsageTextView"
        android:layout_width="124dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="167dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="120dp"
        android:text="@string/my_usage"
        android:textAlignment="center"
        android:textColor="@color/colorDarkText"
        android:textSize="24sp" />

</RelativeLayout>